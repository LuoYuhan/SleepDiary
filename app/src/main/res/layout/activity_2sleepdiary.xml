<?xml version="1.0" encoding="utf-8"?><GridLayout    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#fafafa"    android:icon="@drawable/ic_launcher"    >    <TextView        android:id="@+id/bed"        android:text="I went to bed at:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="0"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="45dp" />    <TextView        android:id="@+id/asleep"        android:text="Time to fall asleep:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="1"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="65dp" />    <TextView        android:id="@+id/woke"        android:text="This morning I Woke up at:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="2"        android:layout_column="0"        android:paddingTop="65dp"        android:layout_marginLeft="40dp" />    <TextView        android:id="@+id/outofbed"        android:text="I finally got out of bed at:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="3"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="65dp"         />    <!--<TextView-->        <!--android:id="@+id/slept"-->        <!--android:text="Last night I actually slept:"-->        <!--android:textSize="28dp"-->        <!--android:textColor="#000000"-->        <!--android:layout_width="wrap_content"-->        <!--android:layout_height="wrap_content"-->        <!--android:layout_row="4"-->        <!--android:layout_column="0"-->        <!--android:layout_marginStart="40dp"-->        <!--android:paddingTop="45dp"-->        <!--/>-->    <TextView        android:id="@+id/nowake"        android:text="Number of Awakeness during last night:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="4"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="65dp"         />    <TextView        android:id="@+id/over"        android:text="Overall, I slept:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="5"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="65dp"        />    <TextView        android:id="@+id/waking"        android:text="At waking, I feel:"        android:textSize="28dp"        android:textColor="#000000"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_row="6"        android:layout_column="0"        android:layout_marginStart="40dp"        android:paddingTop="65dp"        />    <RadioGroup        android:id="@+id/buttongroup"        android:layout_width="472dp"        android:layout_height="180dp"        android:layout_marginStart="40dp"        android:orientation="horizontal"        android:minHeight="100dp"        android:minWidth="200dp"        android:paddingTop="75dp"        android:layout_row="7"        android:layout_column="0">        <Button            android:id="@+id/cancel_s2"            android:width="200dp"            android:height="45dp"            android:text="Cancel"            android:textAllCaps="false"            android:layout_centerHorizontal="true"            android:textSize="28dp"            android:background="#959595"            android:layout_width="139dp"            android:layout_height="wrap_content"            android:onClick="button_SD2dOnClick"            />        <Button            android:id="@+id/save_s2"            android:width="200dp"            android:height="45dp"            android:layout_height="wrap_content"            android:layout_width="wrap_content"            android:text="Save &amp; Continue"            android:textAllCaps="false"            android:layout_marginStart="50dp"            android:layout_centerHorizontal="true"            android:textSize="28dp"            android:background="#959595"            android:layout_weight="4.69"            android:checked="false"            android:onClick="button_SD2dOnClick" />    </RadioGroup>    <Button        android:id="@+id/bedt"        android:width="200dp"        android:height="45dp"        android:text="Pick Time"        android:textAllCaps="false"        android:layout_marginLeft="30dp"        android:layout_centerHorizontal="true"        android:layout_row="0"        android:layout_column="1"        android:textSize="25dp"        android:background="#959595"        android:layout_width="139dp"        />    <Button        android:id="@+id/asleept"        android:width="200dp"        android:height="45dp"        android:text="Pick Time"        android:textAllCaps="false"        android:layout_marginLeft="30dp"        android:layout_centerHorizontal="true"        android:layout_row="1"        android:layout_column="1"        android:textSize="25dp"        android:background="#959595"        android:layout_width="139dp"        />    <Button        android:id="@+id/woket"        android:width="200dp"        android:height="45dp"        android:text="Pick Time"        android:textAllCaps="false"        android:layout_marginLeft="30dp"        android:layout_centerHorizontal="true"        android:layout_row="2"        android:layout_column="1"        android:textSize="25dp"        android:background="#959595"        android:layout_width="139dp"        />    <Button        android:id="@+id/ofbed"        android:width="200dp"        android:height="45dp"        android:text="Pick Time"        android:textAllCaps="false"        android:layout_marginLeft="30dp"        android:layout_centerHorizontal="true"        android:layout_row="3"        android:layout_column="1"        android:textSize="25dp"        android:background="#959595"        android:layout_width="139dp"        />    <!--<Button-->        <!--android:id="@+id/asleeptime"-->        <!--android:width="200dp"-->        <!--android:height="45dp"-->        <!--android:text="Pick Time"-->        <!--android:textAllCaps="false"-->        <!--android:layout_marginLeft="30dp"-->        <!--android:layout_centerHorizontal="true"-->        <!--android:layout_row="4"-->        <!--android:layout_column="1"-->        <!--android:textSize="25dp"-->        <!--android:background="#959595"-->        <!--android:layout_width="139dp"-->        <!--/>-->    <Spinner        android:layout_width="160dp"        android:layout_height="wrap_content"        android:id="@+id/wakeno"        android:entries="@array/number"        android:layout_row="4"        android:layout_column="1"        android:layout_marginRight="80dp"        android:layout_marginLeft="20dp" /></GridLayout>